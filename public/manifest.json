{
  "manifest_version": 2,
  "name": "YouTube Ad Mute",
  "description": "Automatically mutes the volume of YouTube ads. One of the options for distributors.",
  "version": "1.0.3",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "eventpage.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_icon": {
      "19": "icon19.png"
    },
    "default_title": "YouTubeAdVolumeController", 
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.youtube.com/*",
    "storage"
  ],
  "icons": {
    "16":  "icon16.png",
    "48":  "icon48.png",
    "128": "icon128.png"
  }
}